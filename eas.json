{
  "cli": {
    "version": ">= 3.8.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "channel": "development",
      "env": {
        "API_URL": "https://demo.start-ui.com/api/rest",
        "OPEN_API_URL": "https://demo.start-ui.com/api/openapi.json"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "channel": "preview",
      "env": {
        "API_URL": "https://demo.start-ui.com/api/rest",
        "OPEN_API_URL": "https://demo.start-ui.com/api/openapi.json"
      }
    },
    "production": {
      "ios": {
        "resourceClass": "m-medium"
      },
      "channel": "production",
      "env": {
        "API_URL": "https://demo.start-ui.com/api/rest",
        "OPEN_API_URL": "https://demo.start-ui.com/api/openapi.json"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
