{
  "cli": {
    "version": ">= 16.20.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "channel": "development",
      "environment": "development",
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "channel": "preview",
      "environment": "preview",
      "distribution": "internal"
    },
    "production": {
      "channel": "production",
      "environment": "production",
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {
      "ios": {},
      "android": {
        "releaseStatus": "draft"
      }
    }
  }
}
